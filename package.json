{
  "name": "promidas-utils",
  "version": "2.0.0",
  "description": "Utilities to assist development using PROMIDAS",
  "engines": {
    "node": ">=22"
  },
  "private": false,
  "type": "module",
  "keywords": [
    "utilities",
    "promidas",
    "protopedia",
    "nodejs",
    "typescript"
  ],
  "author": "F88 <685250+F88@users.noreply.github.com>",
  "license": "MIT",
  "homepage": "https://github.com/F88/promidas-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/F88/promidas-utils.git"
  },
  "bugs": {
    "url": "https://github.com/F88/promidas-utils/issues"
  },
  "scripts": {
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:codecov": "vitest run --coverage --reporter=default --reporter=junit --outputFile.junit=test-report.junit.xml",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && node scripts/generate-version.mjs",
    "build": "tsc --project tsconfig.build.json",
    "pack": "npm pack",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --project tsconfig.dev.json --noEmit",
    "prepare": "npm run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "promidas": ">=2.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.10.2",
    "@types/webappsec-credential-management": "^0.6.9",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.8.0",
    "promidas": "^2.0.0",
    "rimraf": "^6.1.2",
    "ts-essentials": "^10.1.1",
    "typescript": "^5.9.3",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.17"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./config": {
      "types": "./dist/lib/config/index.d.ts",
      "import": "./dist/lib/config/index.js"
    },
    "./token": {
      "types": "./dist/lib/token/index.d.ts",
      "import": "./dist/lib/token/index.js"
    },
    "./store": {
      "types": "./dist/lib/store/index.d.ts",
      "import": "./dist/lib/store/index.js"
    },
    "./builder": {
      "types": "./dist/lib/builder/index.d.ts",
      "import": "./dist/lib/builder/index.js"
    },
    "./repository": {
      "types": "./dist/lib/repository/index.d.ts",
      "import": "./dist/lib/repository/index.js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "provenance": true
  }
}
